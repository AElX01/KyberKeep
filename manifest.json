{
    "manifest_version": 3,
    "name": "KyberKeep",
    "version": "1.0",
    "description": "Autofill and save passwords securely.",
    "permissions": [
      "storage",
      "activeTab",
      "scripting",
      "identity",
      "tabs"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "views/popup.html",
      "default_icon": {
        "16": "assets/icon/Logo.png",
        "48": "assets/icon/Logo.png",
        "128": "assets/icon/Logo.png"
      }
    }
  }